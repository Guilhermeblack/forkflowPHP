{
    "name": "bemagro/traceflow-php",
    "description": "A PHP library for interacting with the TraceFlow API",
    "type": "library",
    "authors": [
        {
            "name": "Guilherme Pereira",
            "email": "plataforma3@bemagro.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Bemagro\\TraceFlow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bemagro\\TraceFlow\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "repositories": {
        "packagist.org": false,
        "private-packagist": {
            "type": "composer",
            "url": "https://repo.packagist.com/gdev/"
        }
    }
}
